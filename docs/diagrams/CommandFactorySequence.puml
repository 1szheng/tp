@startuml
'https://plantuml.com/sequence-diagram
!include style.puml
skinparam ClassFontColor #000000
skinparam ClassBorderColor #000000

participant Main LOGIC_COLOR_T1
Box " "#WHITE
    participant ":CommandFactory" as CommandFactory COMMAND_COLOR
    participant ":Parser" as Parser LOGIC_COLOR_T2
    participant ":XYZCommand" as XYZCommand COMMAND_COLOR_T2
end Box

-> Main: userInput
activate Main
Main -> CommandFactory: getCommand(userInput, family)
activate CommandFactory
CommandFactory -> Parser: parseCommandTypes(userInput)
activate Parser
Parser --> CommandFactory: keyword
deactivate Parser
create XYZCommand
CommandFactory --> XYZCommand
activate XYZCommand
XYZCommand --> CommandFactory: :XYZCommand
deactivate XYZCommand
CommandFactory --> Main: return :XYZCommand
deactivate CommandFactory
Main -> XYZCommand: execute()
activate XYZCommand
@enduml
