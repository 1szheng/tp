@startuml
!include <office/Concepts/globe_internet>
!include <office/Concepts/documents>
!include <office/Users/user>
!include style.puml

Package " "<<Rectangle>>{
    Class UI UI_COLOR
    Class Commands LOGIC_COLOR
    Class Storage STORAGE_COLOR
    Class Family MODEL_COLOR
    Class MoneyList MODEL_COLOR_T3
    Class Main #grey
    Class Parser LOGIC_COLOR_T2
}

Class "<$user>\nUser" as User MODEL_COLOR_T2
Class "<$documents>\nSaved Data" as File UI_COLOR_T2

Main -right[#grey]->UI
Main -down[#grey]->Family
Main -right[#grey]->Commands
Main -down[#grey]->Storage

Commands -up[#blue]->UI
Commands -left[#blue]->Family
Commands -down[#blue]->Storage
Commands -down[#blue].>Parser

Family -down[MODEL_COLOR]->MoneyList

Storage -left[STORAGE_COLOR].>Family
Storage .down[STORAGE_COLOR].>File

User ..> Main
@enduml

