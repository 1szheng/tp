'@@author tjiarong
@startuml
!include style.puml
skinparam ClassFontColor #000000
skinparam ClassBorderColor #000000
show members
hide empty members

title After iterating Alice's ExpenditureList

package " " {
    class temp as "__tempExpList:ExpenditureList__"
    class Fam as "__family:Family__"
    class MyGen as "__myGen:PersonList__"
    class Alice as "__Alice:Person__"
    class List as "__ExpenditureList:ExpenditureList__"
    class Exp0 as "__Expenditure0:Expenditure__" #LIGHTGREEN {
        description = "Lunch"
        category = 2
    }
    class Exp1 as "__Expenditure1:Expenditure__" #LIGHTGREEN {
        description = "Dinner"
        category = 2
    }
    class Exp2 as "__Expenditure2:Expenditure__" #LIGHTGREEN {
        description = "Movies"
        category = 6
    }
    class Exp3 as "__Expenditure3:Expenditure__" #LIGHTGREEN {
        description = "Electric Bills"
        category = 1
    }
    class Exp4 as "__Expenditure3:Expenditure__" #LIGHTGREEN {
        description = "Electric Bills"
        category = 1
    }
}

Fam -down-> MyGen

MyGen -> Alice

Alice -down-> List

List  -d[#GREEN]-> Exp0
List  -d[#GREEN]-> Exp1
List  -d[#GREEN]-> Exp2
List  -d[#GREEN]-> Exp3

temp  -u[#GREEN]-> Exp4


temp-[HIDDEN]>Fam

Exp0 -right[HIDDEN]> Exp1
Exp1 -right[HIDDEN]> Exp2
Exp2 -right[HIDDEN]> Exp3

@end